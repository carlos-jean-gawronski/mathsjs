{
  "name": "mathsjs",
  "version": "1.0.0",
  "description": "Math helper package",
  "main": "index.js",
  "scripts": {
    "lint": "npx eslint --fix && npx prettier --config ./.prettierrc --write .",
    "test": "jest",
    "compile": "npx tsc"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm test",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.js": [
      "npx eslint --fix",
      "npx prettier --config ./.prettierrc --write .",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/carlos-jean-gawronski/mathsjs.git"
  },
  "keywords": [
    "math",
    "analizer",
    "helper",
    "nodejs",
    "npm"
  ],
  "author": "Carlos Jean Gawronski",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/carlos-jean-gawronski/mathsjs/issues"
  },
  "homepage": "https://github.com/carlos-jean-gawronski/mathsjs#readme",
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/jest": "^25.2.3",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "commitizen": "^4.1.2",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^7.0.0",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "lint-staged": "^10.2.4",
    "prettier": "^2.0.5",
    "ts-jest": "^26.0.0",
    "typescript": "^3.9.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
